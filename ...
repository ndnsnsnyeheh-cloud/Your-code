local function animateLoader(duration)
duration = duration or 2
local tween = TweenService:Create(ProgressBar, TweenInfo.new(duration, Enum.EasingStyle.Linear), {Size=UDim2.new(1,0,1,0)})
tween:Play()
tween.Completed:Wait()
end

